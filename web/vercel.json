{
  "version": 2,
  "outputDirectory": "public",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/forecasts",
      "dest": "/forecasts.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}